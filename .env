# Variables de entorno para JobLink

# Puerto del servidor
PORT=3006

# Configuración de la base de datos
# Puede ser una cadena de conexión completa o parámetros individuales
# DB_URL=mysql://usuario:contraseña@localhost:3306/joblink

# O usar parámetros individuales
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=123456
DB_NAME=joblink

# Secreto para sesiones
SESSION_SECRET=joblink_session_secret_key_change_this

# Modo de entorno (development o production)
NODE_ENV=development

